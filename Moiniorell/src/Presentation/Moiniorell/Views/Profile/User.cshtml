                                            <div class="social-links">
                                            <ul class="header-nav d-flex flex-wrap justify-start p-0 m-0">
                                                <a asp-controller="Profile" asp-action="Follow" asp-route-followeeId="@Model.Id"><li class=" rounded p-2  mr-3"> Follow</li></a>

